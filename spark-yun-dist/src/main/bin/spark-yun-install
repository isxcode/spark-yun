#!/bin/bash

rm -rf ~/spark-yun-agent && mkdir ~/spark-yun-agent
echo "创建spark-yun-agent文件夹"

tar -vxf ~/spark-yun-agent.tar.gz -C ~/spark-yun-agent/
echo "解压文件"

bash ~/spark-yun-agent/bin/spark-yun-start
echo "执行spark-yun-start启动脚本"

rm ~/spark-yun-agent.tar.gz && rm ~/spark-yun-install
echo "安装完成，删除多余的文件"
